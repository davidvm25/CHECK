        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="403dp" />

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
